<!DOCTYPE html>
<html lang="en" ng-app="app" ng-csp ng-controller="AppController" ng-cloak>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
	
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/min.css"/>

    <title>Pickup Gamer</title>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Custom styles for this template -->

  </head>

  <body>
	<div class="container" >
		<ng-include src="partial('shared/nav')"></ng-include>
		<ng-view></ng-view>
    	<ng-include src="partial('shared/footer')"></ng-include>
	</div>

  <!-- cdns -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.3/angular.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.3/angular-route.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.3/angular-resource.min.js"></script>

  <% if(js.length){ %>
    <% js.forEach(function(file){ %>
      <script type="text/javascript" src="/<%= file.replace('app/', '') %>"></script>
    <% }); %>
  <% } else { %>
    <script type="text/javascript" src="/js/min.js"></script>
  <% } %>
  </body>
</html>